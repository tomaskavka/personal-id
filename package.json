{
  "name": "personal-id",
  "version": "1.0.0",
  "description": "Personal identification number validation and utils",
  "main": "personal-id.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cover": "node node_modules/nyc/bin/nyc --require babel-register -r lcov -r text -r html npm run test-node",
    "lint": "eslint src test || true",
    "test-node": "node node_modules/mocha/bin/mocha test -c --compilers js:babel-register || true",
    "test-browser": "grunt dist && node node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test": "npm run test-node && npm run test-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomaskavka/personal-id.git"
  },
  "keywords": [
    "pin",
    "personal",
    "identification",
    "number",
    "validator",
    "validation"
  ],
  "author": "Tomas Kavka (iam@tomaskavka.cz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomaskavka/personal-id/issues"
  },
  "homepage": "https://github.com/tomaskavka/personal-id#readme",
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "karma": "^1.3.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "sinon": "^1.17.6"
  }
}
